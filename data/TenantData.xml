    <!-- Back-End -->
    <TenantDomainName tenantId="shopmax" domainName="shopmaxbackend.ofbizdemo.com" initialPath="/backend"/>
    <TenantDomainName tenantId="shopmax" domainName="shopmaxbackend.localhost" initialPath="/backend"/>
