        jdbcUri="jdbc:derby:shopmaxolap;create=true" jdbcUsername="ofbiz" jdbcPassword="ofbiz"/>
    
    <!-- Front-End -->
    <TenantDomainName tenantId="shopmax" domainName="shopmax.ofbizdemo.com" initialPath="/shopmax"/>
