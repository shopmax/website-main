<?xml version="1.0" encoding="UTF-8"?>
<entity-engine-xml>
    <Content contentId="SHOPMAX_PPOINT" contentTypeId="WEB_SITE_PUB_PT" contentName="ShopMax Site Publish Point" description=""/>
    <WebSiteContent webSiteId="SHOPMAX_CENTRAL" contentId="SHOPMAX_PPOINT" webSiteContentTypeId="PUBLISH_POINT" fromDate="2013-04-01 00:00:00"/>
    
    <!-- Action Decorator -->
    <DataResource dataResourceId="SHOPMAX_ACTION_DEC" dataResourceTypeId="URL_RESOURCE" dataTemplateTypeId="SCREEN_COMBINED" objectInfo="component://shopmax/widget/CommonScreens.xml#action-decorator" />
    <Content contentId="SHOPMAX_ACTION_DEC" contentTypeId="DOCUMENT" contentName="ShopMax Action Decorator" dataResourceId="SHOPMAX_ACTION_DEC"/>
    <ContentPurpose contentId="SHOPMAX_ACTION_DEC" contentPurposeTypeId="SECTION"/>
    
    <!-- Main Decorator -->
    <!--
         GROWERPFES_DEC (Content of type DECORATOR associated to a DataResource with placeholders for mapKeys "htmlHead", "header", "footer")
          |_____(ContentAssoc of type SUB_CONTENT with mapKey "htmlHead")______> GROWERPFES_HHEAD (Content of type DOCUMENT)
          |_____(ContentAssoc of type SUB_CONTENT with mapKey "header")________> GROWERPFES_HEADER (Content of type DOCUMENT)
          |_____(ContentAssoc of type SUB_CONTENT with mapKey "footer")________> GROWERPFES_FOOTER (Content of type DOCUMENT)
    -->
    <DataResource dataResourceId="SHOPMAX_DEC" dataResourceTypeId="ELECTRONIC_TEXT" dataTemplateTypeId="FTL"/>
    <ElectronicText dataResourceId="SHOPMAX_DEC">
        <textData><![CDATA[
        ${(decoratedContent.subcontent.htmlHead)?default((thisContent.subcontent.htmlHead)?if_exists)}
        ${(decoratedContent.subcontent.announce)?default((thisContent.subcontent.announce)?if_exists)}
        ${(decoratedContent.subcontent.header)?default((thisContent.subcontent.header)?if_exists)}
        ${decoratedContent}
        ${(decoratedContent.subcontent.footer)?default((thisContent.subcontent.footer)?if_exists)}
    ]]></textData>
    </ElectronicText>
    <Content contentId="SHOPMAX_DEC" contentTypeId="DECORATOR" contentName="ShopMax Main Decorator" dataResourceId="SHOPMAX_DEC" decoratorContentId="SHOPMAX_ACTION_DEC"/>
    <ContentPurpose contentId="SHOPMAX_DEC" contentPurposeTypeId="SECTION"/>
    <ContentAssoc contentId="SHOPMAX_PPOINT" contentIdTo="SHOPMAX_DEC" contentAssocTypeId="SUB_CONTENT" fromDate="2013-04-01 00:00:00" mapKey="mainDecorator"/>
    
    <!-- WebSiteCMS -->
    <DataResource dataResourceId="WEBSITE_CMS" dataResourceTypeId="ELECTRONIC_TEXT" dataTemplateTypeId="FTL"/>
    <ElectronicText dataResourceId="WEBSITE_CMS">
        <textData><![CDATA[
                ${thisContent.subcontent.leftPanel}
                </div>
                <div id="cmscontent" class="no-clear">
                </div>
                </div>
            </div>
        ]]></textData>
    </ElectronicText>
    <Content contentId="WEBSITE_CMS" contentTypeId="DOCUMENT" decoratorContentId="GROWERPFES_DEC" contentName="WebSite CMS" dataResourceId="WEBSITE_CMS"/>
    <ContentPurpose contentId="WEBSITE_CMS" contentPurposeTypeId="SECTION" />
    <ContentAssoc contentId="SHOPMAX_PPOINT" contentIdTo="WEBSITE_CMS" contentAssocTypeId="SUB_CONTENT" fromDate="2013-04-01 00:00:00" mapKey="main"/>
    <WebSiteContent webSiteId="SHOPMAX_CENTRAL" contentId="WEBSITE_CMS" webSiteContentTypeId="DEFAULT_PAGE" fromDate="2013-04-01 00:00:00"/>
    <WebSitePathAlias webSiteId="SHOPMAX_CENTRAL" pathAlias="main" aliasTo="WEBSITE_CMS" contentId="WEBSITE_CMS" />
    
    <!-- Left Panel -->
    <DataResource dataResourceId="SHOPMAX_LEFTPANEL" dataResourceTypeId="URL_RESOURCE" dataTemplateTypeId="SCREEN_COMBINED" objectInfo="component://shopmax/widget/shopmax/ShopMaxScreen.xml#WebSiteCMSNav" />
    <Content contentId="SHOPMAX_LEFTPANEL" contentTypeId="DOCUMENT" contentName="ShopMax Left Panel" dataResourceId="SHOPMAX_LEFTPANEL"/>
